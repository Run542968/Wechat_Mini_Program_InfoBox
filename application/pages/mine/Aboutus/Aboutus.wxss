.image-intro{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin: 30rpx;
}

.head-image, .member image{
  width: 150rpx;
  height: 150rpx;
  border: 5rpx solid #CAE7B9;
  border-radius: 50%;
  margin: 20rpx;
  background-color: #ffffff;
}

.image-intro text, .group-intro text{
  margin: 20rpx;
  font-size: 24rpx;
  text-indent: 2em;
  letter-spacing: 4rpx;
}

.group-intro text{
  margin: 10rpx 50rpx;
}
.subtitle{
  text-align:center;
  width: 88%;
  background-color: #CAE7B9;
  border-radius: 25rpx;
  margin: 20rpx 40rpx;
  padding: 10rpx;
}

.members{
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 40rpx;
}

.member{
  width: 50%;
  text-align: center;
}

.group-intro{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
